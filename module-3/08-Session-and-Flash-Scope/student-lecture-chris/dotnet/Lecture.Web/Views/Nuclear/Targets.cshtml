@model List<Country>

@section Title {
    Targets
}

<partial name="_NuclearLinks" />

<table>
    <tr>
        <th>Code</th>
        <th>Name</th>
        <th>Continent</th>
        <th>Head Of State</th>
        <th>Population</th>
    </tr>
    @foreach (var country in Model)
{
    <tr>
        <td>@country.Code</td>
        <td>@country.Name</td>
        <td>@country.Continent</td>
        <td>@country.HeadOfState</td>
        <td>@country.Population</td>
    </tr>
}
</table>
